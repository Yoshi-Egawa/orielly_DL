{
	"name": "orielly_DeepLearning",
	"image": "mcr.microsoft.com/devcontainers/python:1-3.11-bookworm",
	"updateContentCommand": ".devcontainer/initCommand.sh",
	"customizations": {
		"vscode": {
			"settings": {
				"python.analysis.extraPaths": ["deep-learning-from-scratch/"],
				"terminal.integrated.env.linux": {
					"PYTHONPATH": "deep-learning-from-scratch/dataset"
				}
			},
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			]
		}
	}
}
